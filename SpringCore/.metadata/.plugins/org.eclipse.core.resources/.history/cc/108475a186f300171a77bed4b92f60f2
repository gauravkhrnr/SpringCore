package com.pack;

import org.springframework.stereotype.Component;


@Component("employee")
public class Employee {

	private static int count = 0;
	public String getName() {
		return name;
	}

	public void setName(String name) {
		this.name = name;
	}

	private String name="gaurav";
	
	
	@Override
	public String toString() {
		
		return name +(count++);
	}
}
