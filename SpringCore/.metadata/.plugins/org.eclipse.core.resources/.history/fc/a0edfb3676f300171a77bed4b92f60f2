package com.pack;

import org.springframework.stereotype.Component;

@Component("employee")
public class Employee {

	public String getName() {
		return name;
	}

	public void setName(String name) {
		this.name = name;
	}

	private String name="gaurav";
	
	
	public void say() {
		
		System.out.println("Hello!...");
	}
}
